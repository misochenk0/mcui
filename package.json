{
  "name": "mediacube-ui",
  "version": "0.3.0",
  "description": "Design system for Mediacube services (Vue 3)",
  "author": "Mediacube",
  "private": false,
  "homepage": "https://mediacube.io",
  "license": "MIT",
  "main": "dist/mediacube-ui.common.js",
  "files": [
    "src/assets",
    "src/helpers",
    "src/styles",
    "src/tokens",
    "src/utils",
    "src/mixins",
    "src/elements/**/*.vue",
    "src/patterns/**/*.vue",
    "src/templates/**/*.vue",
    "src/main.js"
  ],
  "scripts": {
    "start": "yarn scss-export && start-storybook -p 6006 -s public",
    "build-storybook": "storybook build",
    "serve": "yarn scss-export && vue-cli-service serve",
    "build": "yarn scss-export && vue-cli-service build --target lib --inline-vue",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "scss-export": "sass-export src/tokens/*.scss -o src/assets/tokens/tokens.json",
    "release": "yarn build && standard-version",
    "release:patch": "yarn release -- --release-as patch",
    "release:minor": "yarn release -- --release-as minor",
    "release:major": "yarn release -- --release-as major",
    "publish:patch": "yarn release:patch && yarn publish . && git push",
    "publish:minor": "yarn release:minor && yarn publish . && git push",
    "publish:major": "yarn release:major && yarn publish . && git push",
    "storybook": "storybook dev -p 6006",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/*.{vue}": "eslint --fix --ext .vue src/"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "floating-vue": "^2.0.0-beta.22",
    "lodash": "^4.17.15",
    "mitt": "^3.0.0",
    "moment-timezone": "0.5.35",
    "normalize.css": "^8.0.1",
    "portal-vue": "^3.0.0",
    "sass": "^1.63.4",
    "sass-loader": "^13.3.2",
    "vue": "^3.3.4",
    "vue-cropperjs": "^5.0.0",
    "vue-datepicker-next": "^1.0.3",
    "vue-final-modal": "^4.4.2",
    "vue-imask": "6.6.3",
    "vue-multiselect": "^3.0.0-beta.2",
    "vue-slider-component": "^4.1.0-beta.7",
    "vue-toastification": "^1.7.14",
    "vxe-table": "4.4.2",
    "webfontloader": "^1.6.28",
    "xe-utils": "2.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-backgrounds": "^5.3.19",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-essentials": "7.0.22",
    "@storybook/addon-interactions": "7.0.22",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-storyshots": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/blocks": "7.0.22",
    "@storybook/testing-library": "0.0.14-next.2",
    "@storybook/vue": "^5.3.19",
    "@storybook/vue3": "7.0.22",
    "@storybook/vue3-vite": "7.0.22",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-unit-jest": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-vue": "^9.15.0",
    "husky": "^8.0.0",
    "jest-vue-preprocessor": "^1.7.1",
    "lint-staged": "^13.2.2",
    "numeral": "^2.0.6",
    "postcss-import": "12.0.1",
    "prettier": "^2.8.8",
    "sass-export": "^2.1.2",
    "standard-version": "8.0.1",
    "storybook": "7.0.22",
    "storybook-addon-designs": "^6.3.1",
    "storybook-design-token": "^2.9.0",
    "xe-clipboard": "^1.10.2"
  },
  "repository": "git@github.com:misochenk0/mcui.git"
}
